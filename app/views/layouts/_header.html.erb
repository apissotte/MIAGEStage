<%
  evaluation =
    '<li class="nav-item">
      <a class="nav-link" href="/menuetudiant"><p>EVALUATION</p></a>
    </li>'.html_safe
  evolution =
    '<li class="nav-item">
      <a class="nav-link" href="/evolution"><p>EVOLUTION</p></a>
    </li>'.html_safe
  tableaudebord =
    '<li class="nav-item">
      <a class="nav-link" href="/tableaudebord"><p>TABLEAU DE BORD</p></a>
    </li>'.html_safe
%>
<nav class="navbar navbar-expand-lg navbar-light bg-light">
  <a class="navbar-brand" href="#"></a>
  <a href="/"><img class="logo" style="margin-left: 10px; margin-top:-15px;" <%= image_tag(image_url('miage.png'), class: 'image') %>
  </a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation" style="margin-right: 10px;">
    <span class="navbar-toggler-icon"></span>
  </button>

  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="nav navbar-nav navbar-right">


      <% if tuteur_universitaire_signed_in? %>
        <%= tableaudebord %>
        <%= evolution %>
        <li class="nav-item">
          <%= link_to 'Edition profile', edit_tuteur_universitaire_registration_path, :class => 'navbar-link' %>
        </li>
        <li class="nav-item">
          <%= link_to "Deconnexion", destroy_tuteur_universitaire_session_path, method: :delete, :class => 'navbar-link' %>
        </li>
      <% end %>


      <% if etudiant_signed_in? %>
        <%= evaluation %>
        <li class="nav-item">
          <a class="nav-link">
            <p> <%= link_to 'Edition profile', edit_etudiant_registration_path, :class => 'navbar-link' %></p></a>
        </li>
        <li class="nav-item">
          <a class="nav-link">
            <p><%= link_to "Deconnexion", destroy_etudiant_session_path, method: :delete, :class => 'navbar-link' %></p>
          </a>
        </li>
      <% end %>


      <% if responsable_stage_signed_in? %>
        <li class="nav-item">
          <a class="nav-link" href="<%= fiche_stages_path %>"><p>FICHE STAGE</p></a>
        </li>
        <li class="nav-item" style="margin-right: 15px">
          <select class="dropdown dropdownNavBar" onChange="window.location.href=this.value">
            <option selected disabled class="dropdown-item" value="">LISTES</option>
            <option class="dropdown-item" value="<%= promotions_path %>">PROMOTIONS</option>
            <option class="dropdown-item" value="<%= etudiants_path %>">ETUDIANTS</option>
            <option class="dropdown-item" value="<%= entreprises_path %>">ENTREPRISES</option>
            <option class="dropdown-item" value="<%= tuteur_universitaires_path %>">TUTEURS</option>
          </select>
        </li>
        <li class="nav-item">
          <select class="dropdown dropdownNavBar" onChange="window.location.href=this.value">
            <option selected disabled class="dropdown-item" value="">GRILLE</option>
            <option class="dropdown-item" value="/tableaudebord">TABLEAU DE BORD</option>
            <option class="dropdown-item" value="/statistiques">STATISTIQUES</option>
            <option class="dropdown-item" value="/evolution">EVOLUTION</option>
            <option class="dropdown-item" value="/menurespstage">GRILLE D'EVALUATION</option>
          </select>
        </li>
        <li class="nav-item">
          <%= link_to 'Edition profile', edit_responsable_stage_registration_path, :class => 'navbar-link' %>
        </li>
        <li class="nav-item">
          <%= link_to "Deconnexion", destroy_responsable_stage_session_path, method: :delete, :class => 'navbar-link' %>
        </li>
      <% end %>


    </ul>
  </div>

</nav>
